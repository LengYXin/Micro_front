  "scripts": {
    "start": "lerna run start --stream",
    "start:www": "lerna run start --stream --scope @xt/www",
    "start:mobile": "lerna run start --stream --scope @xt/mobile",
    "start:live": "lerna run start --stream --scope @xt/live",
    "build": "lerna run build --stream && xt-suture",
    "build:test": "lerna run build:test --stream  && xt-suture test",
    "build:uat": "lerna run build:uat --stream  && xt-suture uat",
    "build:pro": "lerna run build:pro --stream  && xt-suture pro",
    "build:all": "npm run build && npm run build:test && npm run build:uat && npm run build:pro",
    "serve": "lerna run serve --stream",
    "init": "lerna bootstrap",
    "docs": "docsify serve docs",
    "clean": "lerna clean",
    "clean:all": "npm run clean && rimraf node_modules",
    "publish": "lerna publish --skip-npm",
    "test": "xt-suture dev"
